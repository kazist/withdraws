    "query": {
        "from": {
            "table": "Withdraws\\Withdraws\\Code\\Tables\\Withdraws",
            "alias": "ww",
            "name": "#__withdraws_withdraws"
        },
        "select": [
            "ww.id",
            "ww.user_id",
            "uu.name AS user_id_name",
            "ww.gateway_id",
            "pg.long_name AS gateway_id_long_name",
            "ww.amount",
            "ww.currency",
            "ww.description",
            "ww.paid_status",
            "ww.is_canceled",
            "ww.token",
            "ww.params",
            "ww.created_by",
            "uu_631.name AS created_by_name",
            "ww.date_created",
            "ww.modified_by",
            "uu_7020.name AS modified_by_name",
            "ww.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ww.user_id = uu.id"
            },
            {
                "table": "Payments\\Gateways\\Code\\Tables\\Gateways",
                "alias": "pg",
                "name": "#__payments_gateways",
                "on": "ww.gateway_id = pg.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_631",
                "name": "#__users_users",
                "on": "ww.created_by = uu_631.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_7020",
                "name": "#__users_users",
                "on": "ww.modified_by = uu_7020.id"
            }
        ]
    }
