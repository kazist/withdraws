    "query": {
        "from": {
            "table": "Withdraws\\Settings\\Gateways\\Code\\Tables\\Gateways",
            "alias": "wsg",
            "name": "#__withdraws_settings_gateways"
        },
        "select": [
            "wsg.id",
            "wsg.user_id",
            "uu.name AS user_id_name",
            "wsg.gateway_id",
            "pg.long_name AS gateway_id_long_name",
            "wsg.setting_id",
            "ws.pin AS setting_id_pin",
            "ws.id_passport AS setting_id_id_passport",
            "wsg.params",
            "wsg.is_valid",
            "wsg.is_default",
            "wsg.published",
            "wsg.created_by",
            "wsg.date_created",
            "wsg.modified_by",
            "wsg.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "wsg.user_id = uu.id"
            },
            {
                "table": "Payments\\Gateways\\Code\\Tables\\Gateways",
                "alias": "pg",
                "name": "#__payments_gateways",
                "on": "wsg.gateway_id = pg.id"
            },
            {
                "table": "Withdraws\\Settings\\Code\\Tables\\Settings",
                "alias": "ws",
                "name": "#__withdraws_settings",
                "on": "wsg.setting_id = ws.id"
            }
        ]
    }
