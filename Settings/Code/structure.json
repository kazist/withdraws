    "query": {
        "from": {
            "table": "Withdraws\\Settings\\Code\\Tables\\Settings",
            "alias": "ws",
            "name": "#__withdraws_settings"
        },
        "select": [
            "ws.id",
            "ws.user_id",
            "uu.name AS user_id_name",
            "ws.id_passport",
            "ws.pin",
            "ws.created_by",
            "uu_8816.name AS created_by_name",
            "ws.date_created",
            "ws.modified_by",
            "uu_5822.name AS modified_by_name",
            "ws.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ws.user_id = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8816",
                "name": "#__users_users",
                "on": "ws.created_by = uu_8816.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5822",
                "name": "#__users_users",
                "on": "ws.modified_by = uu_5822.id"
            }
        ]
